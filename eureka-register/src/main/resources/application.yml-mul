spring:
  application:
    name: eureka-register
eureka:
  server:
    enable-self-preservation: false
  instance:
    prefer-ip-address: true
logging:
  level:
    root: debug
---
spring:
  profiles: 8761
server:
  port: 8761
eureka:
  instance:
    hostname: 8761
  client:
    service-url:
      defaultZone: http://root:root@localhost:8762/eureka/,http://root:root@localhost:8763/eureka/
